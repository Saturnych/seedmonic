{
	"name": "seedmonic",
	"version": "0.0.1",
	"description": "bip39 wordlists & search",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3030 --host",
		"dev:open": "vite dev --port 3030 --host --open",
		"start": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --port 3030 --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Saturnych/seedmonic.git"
	},
	"bugs": {
		"url": "https://github.com/Saturnych/seedmonic/issues"
	},
	"homepage": "https://seedmonic.ru/",
	"keywords": [
		"sveltekit",
		"typescript",
		"bip39"
	],
	"author": "Saturnych",
	"license": "ISC",
	"devDependencies": {
		"@iconify/svelte": "^4.0.2",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.16",
		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
	},
	"type": "module"
}
